<h1>New Org</h1>

<%= form_for :orgs, url: orgs_path do |f| %>
    <p>
        <%= f.label :name %><br>
        <%= f.text_field :name %>
    </p>
    
    <p>
        <%= f.label :description %><br>
        <%= f.text_field :description %>
    </p>
    
    <p>
        <%= f.label :contact %><br>
        <%= f.text_field :contact %>
    </p>
    
    <p>
        <%= f.label :rating %><br>
        <%= f.text_field :rating %>
    </p>
    
    <p>
        <%= f.submit %>
    </p>
<% end %>